{
  "name": "Node DevContainer",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspace",
  "forwardPorts": [3100],
  "remoteUser": "node",
  "customizations": {
    "vscode": {
      "files.associations": {
        "*.css": "tailwindcss"
      },
      "extensions": [
        "dbaeumer.vscode-eslint",
        "mhutchie.git-graph",
        "esbenp.prettier-vscode",
        "bradlc.vscode-tailwindcss",
        "Vue.volar"
      ]
    }
  }
}
